@model Webzine.ViewModels.SidebarViewModel

<div class="col-12">
	<h2>A propos</h2>
	<p>Retrouvez les dernières pépites sur notre webzine</p>
	<h2>Artistes</h2>
    <ul class="col-12">
        
        @foreach (var item in Model.ArtistesRefs)
        {
            <li class="col-xxl-12 col-xl-12 col-xl-12 col-md-12 col-sm-3">
                @Html.ActionLink(item.Nom, "artiste", "home", new { id = item.IdArtiste }, new { })
            </li>
        }
            
    </ul>
</div>